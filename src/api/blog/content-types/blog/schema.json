{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "author": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "article_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "featured": {
      "type": "boolean",
      "default": true
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "blocks.paragraph",
        "blocks.paragraph-with-image",
        "blocks.full-image"
      ]
    },
    "theme": {
      "type": "enumeration",
      "enum": [
        "primary",
        "warming",
        "soothing",
        "[#B2C2EA]"
      ]
    },
    "read_minutes": {
      "type": "string"
    },
    "tag": {
      "type": "string"
    },
    "mobileStyle": {
      "type": "string"
    }
  }
}
